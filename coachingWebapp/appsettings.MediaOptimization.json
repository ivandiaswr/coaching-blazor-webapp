{
    "MediaOptimization": {
        "EnableLazyLoading": true,
        "EnableResponseCompression": true,
        "VideoSettings": {
            "DefaultQuality": "medium",
            "SupportedFormats": [
                "mp4",
                "webm"
            ],
            "MaxBitrate": {
                "low": "400k",
                "medium": "800k",
                "high": "1200k"
            },
            "Resolutions": {
                "low": "640x360",
                "medium": "1280x720",
                "high": "1920x1080"
            }
        },
        "ImageSettings": {
            "DefaultFormat": "webp",
            "FallbackFormat": "jpg",
            "Quality": {
                "thumbnail": 80,
                "medium": 75,
                "large": 70
            },
            "Sizes": {
                "thumbnail": 300,
                "medium": 800,
                "large": 1200
            }
        },
        "CachingSettings": {
            "VideoCacheMaxAge": 31536000,
            "ImageCacheMaxAge": 15552000,
            "StaticFilesCacheMaxAge": 2592000
        }
    }
}